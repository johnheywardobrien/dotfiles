[user]
	name  = <%=  print(" - Your Name: ");  STDOUT.flush; STDIN.gets.chomp %>
	email = <%= print(" - Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[credential]
	helper = osxkeychain
[color]
	ui=true
[core]
	excludesfile = ~/.gitignore
[push]
	default = current
[instaweb]
	local = true
	httpd = webrick
[alias]
	s   = status --short --branch
	l   = log
	ll  = log -p
	who = shortlog -s
	d   = diff --color
	dw  = diff --color-words
	dlt = diff lib/ test/
	a   = add
	ap  = app -p
	aa  = add --all .
	alt = add lib/ test/
	c   = commit
	cm  = commit -m
	k   = checkout
	kb  = checkout -b
	b   = branch -a -v
	bd  = branch -d
	bdd = branch -D
	m   = merge
	p   = push
	pt  = push --tags
	u   = pull --ff-only
	f   = fetch
	rb  = rebase
	chp = cherry-pick
	pn  = remote prune origin
	iw  = instaweb
	iws = instaweb --stop
